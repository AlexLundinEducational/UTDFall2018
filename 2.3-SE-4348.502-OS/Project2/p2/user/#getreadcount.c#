#include "types.h"
#include "user.h"
#include "syscall.h"
int
main(int argc, char *argv[])
{
  extern int readcount;
  sys_getreadcount();
 
  printf(1, "\nHello I am '%s'\n,", argv[0]);
  printf(1, "\n\tThe current read count is:\n\t", readcount);

  printf(1, "\nHello I am '%s'\n,", argv[0]);
  printf(1, "\n\tThe current read count is:\n\t", readcount);

  printf(1, "\nHello I am '%s'\n,", argv[0]); 
  printf(1, "\n\tThe current read count is:\n\t", readcount); 
  exit();
}
